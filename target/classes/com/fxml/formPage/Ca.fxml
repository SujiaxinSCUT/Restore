<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1280.0" style="-fx-background-color: F3F3FA;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fxml.controller.CaController">
   <children>
      <HBox alignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label fx:id="mes" alignment="CENTER" opacity="0.42" prefHeight="40.0" prefWidth="600.0" textAlignment="CENTER" visible="false" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="460.0" layoutY="20.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <BorderPane fx:id="container" prefWidth="1000.0" stylesheets="@stylesheet.css" AnchorPane.leftAnchor="460.0" AnchorPane.rightAnchor="460.0" AnchorPane.topAnchor="20.0">
               <top>
                  <VBox spacing="10.0">
                     <children>
                        <HBox fx:id="Top" alignment="CENTER_LEFT" prefHeight="40.0" BorderPane.alignment="CENTER">
                           <children>
                              <Label lineSpacing="10.0" text="  CA  集合" textFill="WHITE">
                                 <font>
                                    <Font name="System Bold" size="18.0" />
                                 </font>
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                                 <HBox.margin>
                                    <Insets left="20.0" />
                                 </HBox.margin>
                              </Label>
                              <HBox alignment="CENTER" spacing="10.0">
                                 <children>
                                    <Button fx:id="UpdateButton" mnemonicParsing="false" onAction="#Update" styleClass="btn-info" text="Update">
                                       <HBox.margin>
                                          <Insets left="650.0" />
                                       </HBox.margin>
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                    </Button>
                                    <Button fx:id="NewButton" mnemonicParsing="false" onAction="#New" text="New">
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                    </Button>
                                    <Button fx:id="DeleteButton" mnemonicParsing="false" text="Delete">
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                    </Button>
                                 </children>
                              </HBox>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </top>
               <center>
                  <VBox spacing="10.0">
                     <children>
                        <TableView fx:id="CaInfo" editable="true" prefHeight="250.0" prefWidth="1000.0" stylesheets="@../css/tableview.css" BorderPane.alignment="CENTER">
                           <columns>
                              <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
                              <TableColumn fx:id="name" prefWidth="110.0" text="name" />
                              <TableColumn fx:id="admin" prefWidth="208.0" text="admin" />
                              <TableColumn fx:id="location" prefWidth="174.0" text="location" />
                              <TableColumn fx:id="adminpw" prefWidth="125.0" text="adminpw" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <StackPane prefHeight="30.0">
                           <children>
                              <Button fx:id="lastPage" mnemonicParsing="false" text="上一页" StackPane.alignment="TOP_LEFT">
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                                 <StackPane.margin>
                                    <Insets left="20.0" />
                                 </StackPane.margin>
                              </Button>
                              <Button fx:id="nextPage" mnemonicParsing="false" text="下一页" StackPane.alignment="TOP_RIGHT">
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                                 <StackPane.margin>
                                    <Insets right="20.0" />
                                 </StackPane.margin>
                              </Button>
                           </children>
                        </StackPane>
                     </children>
                  </VBox>
               </center>
               <HBox.margin>
                  <Insets top="50.0" />
               </HBox.margin>
            </BorderPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
