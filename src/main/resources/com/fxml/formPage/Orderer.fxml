<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="1000.0" prefWidth="1280.0" style="-fx-background-color: F3F3FA;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fxml.controller.OrdererController">
   <children>
      <HBox alignment="CENTER" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label fx:id="mes" alignment="CENTER" opacity="0.42" prefHeight="40.0" prefWidth="600.0" textAlignment="CENTER" visible="false" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="460.0" layoutY="20.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="20.0">
         <children>
            <BorderPane fx:id="container" prefWidth="1000.0" stylesheets="@stylesheet.css" AnchorPane.leftAnchor="460.0" AnchorPane.rightAnchor="460.0" AnchorPane.topAnchor="20.0">
               <top>
                  <VBox spacing="10.0">
                     <children>
                        <HBox fx:id="Top" alignment="CENTER_LEFT" prefHeight="40.0" BorderPane.alignment="CENTER">
                           <children>
                              <Label lineSpacing="10.0" text="排序服务集合" textFill="WHITE">
                                 <font>
                                    <Font name="System Bold" size="18.0" />
                                 </font>
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                                 <HBox.margin>
                                    <Insets left="20.0" />
                                 </HBox.margin>
                              </Label>
                              <HBox alignment="CENTER" spacing="10.0">
                                 <children>
                                    <Button fx:id="UpdateButton" mnemonicParsing="false" onAction="#Update" text="Update">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                    </Button>
                                    <Button fx:id="NewButton" mnemonicParsing="false" onAction="#New" text="New">
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                    </Button>
                                    <Button fx:id="DeleteButton" mnemonicParsing="false" text="Delete">
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                       <HBox.margin>
                                          <Insets right="20.0" />
                                       </HBox.margin>
                                    </Button>
                                 </children>
                                 <HBox.margin>
                                    <Insets left="600.0" />
                                 </HBox.margin>
                              </HBox>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </top>
               <center>
                  <VBox spacing="10.0">
                     <children>
                        <TableView fx:id="ordererInfo" editable="true" prefHeight="300.0" prefWidth="1000.0" stylesheets="@../css/tableview.css" BorderPane.alignment="CENTER">
                           <columns>
                              <TableColumn fx:id="id" prefWidth="52.0" text="id" />
                              <TableColumn fx:id="name" prefWidth="187.0" text="名称" />
                              <TableColumn fx:id="leagueName" prefWidth="183.0" text="联盟名称" />
                              <TableColumn fx:id="location" prefWidth="207.0" text="地址" />
                              <TableColumn fx:id="domain_suffix" prefWidth="213.0" text="domain_suffix" />
                              <TableColumn fx:id="Date" prefWidth="157.0" text="日期" />
                           </columns>
                        </TableView>
                        <StackPane prefHeight="30.0">
                           <children>
                              <Button fx:id="lastPage" mnemonicParsing="false" text="上一页" StackPane.alignment="TOP_LEFT">
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                                 <StackPane.margin>
                                    <Insets left="20.0" />
                                 </StackPane.margin>
                              </Button>
                              <Button fx:id="nextPage" mnemonicParsing="false" text="下一页" StackPane.alignment="TOP_RIGHT">
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                                 <StackPane.margin>
                                    <Insets right="20.0" />
                                 </StackPane.margin>
                              </Button>
                           </children>
                        </StackPane>
                     </children>
                  </VBox>
               </center>
               <HBox.margin>
                  <Insets top="50.0" />
               </HBox.margin>
            </BorderPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
